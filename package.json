{
  "name": "dms_api",
  "version": "1.0.0",
  "description": "An Application Programming interface for document management",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./server/server.js",
    "dev": "NODE_ENV=test ./node_modules/gulp/bin/gulp.js develop_server",
    "seed": "./node_modules/.bin/babel-node server/test/helpers/seed.helper.js",
    "mocha": "mocha --compilers js:babel-core/register server/test/*.spec.js",
    "test": "NODE_ENV=test npm run seed && npm run mocha",
    "watch": "mocha --compilers js:babel-core/register server/test/*.spec.js --watch",
    "test:watch": "NODE_ENV=test npm run seed && npm run watch",
    "cover": "NODE_ENV=test npm run seed && rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec server/test",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "migration": "sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-tisrael/checkpoint-document-management.git"
  },
  "keywords": [
    "api",
    "management"
  ],
  "author": "Israel Tomilayo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-tisrael/checkpoint-document-management/issues"
  },
  "homepage": "https://github.com/andela-tisrael/checkpoint-document-management#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "cover": "^0.2.9",
    "coveralls": "^2.11.16",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faker": "^3.1.0",
    "fm-log": "^4.4.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "superagent": "^3.4.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "jsonwebtoken": "^7.2.1",
    "morgan": "^1.7.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.1",
    "sequelize-cli": "^2.5.1"
  }
}
